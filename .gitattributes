#cwe 880185 - Party Scandal - Market Communist
country_event = {
	id = 880185
	title = "EVTNAME880185"
	desc = "EVTDESC880185"
	picture = "news"
	
	trigger = {
		civilized = yes
		is_ideology_enabled = communist_social
		NOT = {
			publishing_rights = censored_publishing
			political_parties = parties_not_allowed
			has_country_modifier = ruling_party_scandal
			has_country_modifier = party_scandal_evidence
		}
		OR = {
			communist_social = 10
			any_owned_province = { communist_social = 20 }
		}
	}
	
	mean_time_to_happen = {
		months = 400
		modifier = {
			factor = 0.75
			election = yes
		}
		modifier = {
			factor = 1.5
			ruling_party_ideology = communist_social
			NOT = { publishing_rights = unregulated_publishing }
		}
		modifier = {
			factor = 0.5
			election = yes
			ruling_party_ideology = communist_social
		}
		modifier = {
			factor = 1.5
			NOT = { average_consciousness = 2 }
		}
		modifier = {
			factor = 1.5
			NOT = { average_consciousness = 4 }
		}
		modifier = {
			factor = 0.8
			average_consciousness = 6
		}
		modifier = {
			factor = 0.8
			average_consciousness = 8
		}
		modifier = {
			factor = 0.8
			average_consciousness = 9
		}
		modifier = {
			factor = 0.8
			communist_social = 25
		}
		modifier = {
			factor = 0.8
			communist_social = 50
		}
		modifier = {
			factor = 0.8
			communist_social = 75
		}	
		modifier = {
			factor = 1.2
			NOT = {	ruling_party_ideology = communist_social }
		}
		modifier = {
			factor = 0.7
			NOT = { ruling_party_ideology = communist_social }
			political_parties = parties_only_nominated
		}
		modifier = {
			factor = 0.8
			NOT = { ruling_party_ideology = communist_social }
			political_parties = parties_allowed
		}
		modifier = {
			factor = 0.9
			NOT = { ruling_party_ideology = communist_social }
			political_parties = parties_promote_democratic
		}
		modifier = {
			factor = 0.9
			NOT = { ruling_party_ideology = communist_social }
			publishing_rights = firewall_publishing
		}
		modifier = {
			factor = 1.5
			ruling_party_ideology = communist_social
			political_parties = parties_only_nominated
		}
		modifier = {
			factor = 1.2
			ruling_party_ideology = communist_social
			political_parties = parties_allowed
		}
		modifier = {
			factor = 1.1
			ruling_party_ideology = communist_social
			political_parties = parties_promote_democratic
		}
		modifier = {
			factor = 0.5
			ruling_party_ideology = communist_social
			has_country_modifier = massive_government_corruption
		}
		modifier = {
			factor = 0.7
			ruling_party_ideology = communist_social
			has_country_modifier = large_government_corrupttion
		}
		modifier = {
			factor = 0.9
			ruling_party_ideology = communist_social
			has_country_modifier = medium_government_corruption
		}
		modifier = {
			factor = 0.9
			ruling_party_ideology = communist_social
			has_country_modifier = mnc_corruption
		}
		modifier = {
			factor = 0.8
			ruling_party_ideology = communist_social
			has_country_modifier = mnc_corrupt_leadership
		}
	}
	
	option = {
