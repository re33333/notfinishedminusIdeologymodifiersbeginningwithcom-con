#cweGeneric Corruption Scandal
country_event = {
	id = 1610039
title = "Major Corruption Scandal"
	desc = EVTDESC1610039
	picture = "major_corruption_scandal"

trigger = {

NOT = { has_country_modifier = leadership_chaos }
NOT = { has_country_modifier = ruling_party_scandal }

OR = { 
has_country_modifier = massive_government_corruption has_country_modifier = large_government_corruption has_country_modifier = medium_government_corruption has_country_modifier = low_government_corruption 
}

OR = { government = democracy government = democracy1 government = hms_government government = hms_government1 government = sar_government government = mandate_government government = socialist_democracy }
}

mean_time_to_happen = { 
months = 12
modifier = { factor = 2 OR = { has_country_modifier = large_government_corruption has_country_modifier = large_societal_corruption } }
modifier = { factor = 3 OR = { has_country_modifier = medium_government_corruption has_country_modifier = medium_government_corruption } }
modifier = { factor = 4 OR = { has_country_modifier = low_government_corruption has_country_modifier = low_government_corruption } }

modifier = { factor = 3 has_country_modifier = nepohigh }
modifier = { factor = 2 has_country_modifier = nepomedium }
modifier = { factor = 0.5 election = yes }

modifier = { factor = 3 publishing_rights = censored_publishing }

modifier = { factor = 2 publishing_rights = firewall_publishing }

modifier = { factor = 2 NOT = { average_militancy = 3 } }

}

option = {
name = "Cover it up!"

random = {
	chance = 35
add_country_modifier = { name = ruling_party_scandal duration = 365 }
}

add_country_modifier = { name = leadership_chaos duration = 10 }

add_tax_relative_income = -25

ai_chance = { factor = 0.75 }
		}

option = {
name = "Admit our mistake!"

prestige = -25

add_country_modifier = { name = leadership_chaos duration = 60 }

