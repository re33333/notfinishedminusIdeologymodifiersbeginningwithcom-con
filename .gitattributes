			#Majority races are angry
			any_pop = {
				limit = { is_culture_group = THIS }
				militancy = 2
				consciousness = 2
			}
			
			#Minority races are pleased
			any_pop = {
				limit = { NOT = { is_culture_group = THIS } }
				militancy = -2
				consciousness = -2
			}
			
			#Progressive and liberals are happy
			any_pop = {
				scaled_militancy = {
					factor = -2
					ideology = liberal
				}
				scaled_militancy = {
					factor = -2
					ideology = progressive
				}
			}
			
			#Gain positive international attention
			any_country = {
				limit = { is_vassal = no citizenship_policy = full_citizenship }
				relation = { who = THIS value = 10 }
			}
			
			#Reduce ideology support from culture group
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { is_culture_group = THIS } ideology = { factor = -0.04 value = communist_social } } } }
			
			#Boost ideology support for non culture groups
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { NOT = { is_culture_group = THIS } } ideology = { factor = 0.04 value = communist_social } } } }
			
		}
		ai_will_do = { 
			factor = 1 
		}
	}
		#cweCampaign for national religion
	campaigning_national_religion = {
		picture = "campaigning_national_religion"

		potential = {
			has_country_flag = show_campaigning_decision
			ai = no
			
			#Every gov type can do this
			
			#Country is not secular
			has_country_flag = desecularized
			
			#Religious party
			religious_policy = moralism
			
			#Religion is less of a politicised issue in highly educated nations
			NOT = { literacy = 0.20 }
		}
		allow = {
			NOT = { has_country_modifier = party_campaigning_timer }
		}
		effect = {
		
			#Timer
			random_owned = {
				limit = { owner = { election = no } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 182 } }
			}
			random_owned = {
				limit = { owner = { election = yes } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 50 } }
			}
			
			#State religion pops are happy
			any_pop = {
				limit = { is_state_religion = yes }
				scaled_militancy = {
					factor = -2
					issue = moralism
				}
				scaled_consciousness = {
					factor = -2
					issue = moralism
				}
			}
			
			#Non state religion pops are angry
			any_pop = {
				limit = { is_state_religion = no }
				scaled_militancy = {
					factor = 2
					issue = secularized
				}
				scaled_consciousness = {
					factor = 2
					issue = secularized
				}
				
				scaled_militancy = {
					factor = 2
					issue = pro_atheism
				}
				scaled_consciousness = {
					factor = 2
					issue = pro_atheism
				}
			}
			
			#Progressive and liberals are not happy
			any_pop = {
				scaled_militancy = {
					factor = 2
					ideology = liberal
				}
				scaled_militancy = {
					factor = 2
					ideology = progressive
				}
			}
			
			#Gain negative international attention
			any_country = {
				limit = { is_vassal = no OR = { religious_policy = pro_atheism religious_policy = secularized } }
				relation = { who = THIS value = -10 }
			}
			
			#Boost ideology support from culture group
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { is_state_religion = yes } ideology = { factor = 0.04 value = communist_social } } } }
			
			#Reduce ideology support for non culture groups
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { is_state_religion = no } ideology = { factor = -0.04 value = communist_social } } } }
			
		}
		ai_will_do = { 
			factor = 1 
		}
	}
		#cweCampaign for secularism
	campaigning_secularism = {
		picture = "campaigning_secularism"

		potential = {
			has_country_flag = show_campaigning_decision
			ai = no
			
			#Every gov type can do this
			
			#Country is not secular
			NOT = { has_country_flag = desecularized }
			
			#Religious party
			OR = { religious_policy = secularized religious_policy = pro_atheism }
			
			#Religion is less of a politicised issue in highly educated nations
			NOT = { literacy = 0.20 }
		}
		allow = {
			NOT = { has_country_modifier = party_campaigning_timer }
		}
		effect = {
		
			#Timer
			random_owned = {
				limit = { owner = { election = no } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 182 } }
			}
			random_owned = {
				limit = { owner = { election = yes } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 50 } }
			}
			
			#State religion pops are happy (Secularism)
			any_pop = {
				limit = { is_state_religion = yes }
				scaled_militancy = {
					factor = -2
					issue = secularized
				}
				scaled_consciousness = {
					factor = -2
					issue = secularized
				}
				
				scaled_militancy = {
					factor = -2
					issue = pro_atheism
				}
				scaled_consciousness = {
					factor = -2
					issue = pro_atheism
				}
			}
			
			#Non state religion pops are angry
			any_pop = {
				limit = { is_state_religion = no }
				scaled_militancy = {
					factor = 2
					issue = moralism
				}
				scaled_consciousness = {
					factor = 2
					issue = moralism
				}
			}
			
			#Progressive and liberals are happy
			any_pop = {
				scaled_militancy = {
					factor = -1
					ideology = liberal
				}
				scaled_militancy = {
					factor = -1
					ideology = progressive
				}
			}
			
			#Boost ideology support from culture group
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { has_pop_religion = secularism } ideology = { factor = 0.02 value = communist_social } } } }
			
			#Reduce ideology support for non culture groups
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { NOT = { has_pop_religion = secularism } } ideology = { factor = -0.04 value = communist_social } } } }
			
		}
		ai_will_do = { 
			factor = 1 
		}
	}
		#cweDictatorship bribes - Rich Strata
	campaigning_dictatorship_rich = {
		picture = "campaigning_dictatorship_rich"

		potential = {
			has_country_flag = show_campaigning_decision
			ai = no
			
			#Dictatorships only
			NOT = { OR = { government = socialist_democracy government = democracy government = democracy1 government = hms_government government = hms_government1 government = sar_government government = colonial_government government = mandate_government } }
			
			#Special ideology limitations
			NOT = { OR = { ruling_party_ideology = communist ruling_party_ideology = communist_social } }
		}
		allow = {
			NOT = { has_country_modifier = party_campaigning_timer }
		}
		effect = {
			add_country_modifier = { name = party_campaigning_timer duration = 182 }
			
			#Poor and middle get angry
			middle_strata = {
				militancy = 1
				consciousness = 1
			}
			poor_strata = {
				militancy = 2
				consciousness = 2
			}
			
			#Boost ideology support for the rich
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { strata = rich } ideology = { factor = 0.04 value = communist_social } } } }
			
			#Reduce ideology support for the poor
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.04 value = communist_social } } } }
			
		}
		ai_will_do = { 
			factor = 1 
		}
	}
