	option = {
		name = "Crack down on the communists."
		owner = { plurality = -2 }
		random_pop = {
			limit = {
				strata = poor
				OR = {
					socialist = 20
					communist = 10
				}
			}
			reduce_pop = 0.98
			militancy = 4
		}
		poor_strata = {
			militancy = 2
		}
		rich_strata = {
			ideology = {
				value = socialist
				factor = -0.1
			}
			ideology = {
				value = communist
				factor = -0.2
			}
			ideology = {
				value = reactionary
				factor = 0.2
			}
			militancy = -3
			consciousness = -3
		}	
	}
	
	option = {
		name = "Let them demonstrate."
		state_scope = {
			any_pop = {
				limit = {
					OR = {
						type = artisans
