country_event = {
	id = 1610032
	title = "$MONARCHTITLE$ is Impeached!"
	desc = EVTDESC1610032
	picture = "impeachment"

trigger = {
OR = { government = democracy government = democracy1 government = socialist_democracy }

#Only presidential or semi-presidential republics
OR = {
	has_country_flag = presidential_system_republic
	has_country_flag = semi_presidential_system_republic
}

election = no
war = no
OR = { has_country_modifier = ruling_party_scandal has_country_modifier = party_scandal_evidence AND = { has_country_modifier = martial_law NOT = { average_militancy = 3 } } }
average_militancy = 1

NOT = { has_country_modifier = headless_chicken }
NOT = { has_country_modifier = leader_cooldown }

OR = { 

AND = { 
ruling_party_ideology = traditionalist 
NOT = { upper_house = { ideology = traditionalist value = 0.33 } }
NOT = { pop_majority_ideology = traditionalist }
}

AND = { 
ruling_party_ideology = conservative
NOT = { upper_house = { ideology = conservative value = 0.33 } }
NOT = { pop_majority_ideology = conservative }
}

