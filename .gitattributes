	#CWECampaign for middle class's interests
	campaigning_middle_class = {
		picture = "campaigning_middle_class"

		potential = {
			has_country_flag = show_campaigning_decision
			ai = no
			
			#Democracies only
			OR = { government = socialist_democracy government = democracy government = democracy1 government = hms_government government = hms_government1 government = sar_government government = colonial_government government = mandate_government }
			
			#Has the required pop type
			has_pop_type = clerks
			
			#Special ideology limitations
			NOT = { OR = { ruling_party_ideology = communist ruling_party_ideology = communist_social } }
		}
		allow = {
			NOT = { has_country_modifier = party_campaigning_timer }
		}
		effect = {
		
			#Timer
			random_owned = {
				limit = { owner = { election = no } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 182 } }
			}
			random_owned = {
				limit = { owner = { election = yes } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 50 } }
			}
			
			#Boost ideology support for the middle class
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { strata = middle } ideology = { factor = 0.04 value = communist_social } } } }
			
			#Reduce ideology support for the lower class
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { strata = poor } ideology = { factor = -0.02 value = communist_social } } } }
		}
		ai_will_do = { 
			factor = 1 
		}
	}
	#cweCampaign for city interests
	campaigning_city_class = {
		picture = "campaigning_city_class"

		potential = {
			has_country_flag = show_campaigning_decision
			ai = no
			
			#Every gov type can do this
			
			#Has the required province type
			any_owned_province = { is_state_capital = yes }
			
		}
		allow = {
			NOT = { has_country_modifier = party_campaigning_timer }
		}
		effect = {
		
			#Timer
			random_owned = {
				limit = { owner = { election = no } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 182 } }
			}
			random_owned = {
				limit = { owner = { election = yes } }
				owner = { add_country_modifier = { name = party_campaigning_timer duration = 50 } }
			}
			
			#Boost ideology support in cities
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = liberal } } } }
