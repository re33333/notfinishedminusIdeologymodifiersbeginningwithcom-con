			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = communist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist_social } } owner = { any_pop = { limit = { location = { is_state_capital = yes } } ideology = { factor = 0.04 value = communist_social } } } }
			
			#Reduce ideology support in rural towns
			random_owned = { limit = { owner = { ruling_party_ideology = populist } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = populist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = nationalist } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = nationalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = traditionalist } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = traditionalist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = conservative } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = conservative } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = liberal } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = liberal } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = big_tent } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = big_tent } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = progressive } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = progressive } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = socialist } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = socialist } } } }
			random_owned = { limit = { owner = { ruling_party_ideology = communist } } owner = { any_pop = { limit = { location = { is_state_capital = no } } ideology = { factor = -0.04 value = communist } } } }
