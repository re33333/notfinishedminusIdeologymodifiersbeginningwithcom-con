#cwe 880190 - Party Scandal - Islamist #cwe Fascist
country_event = {
	id = 880190
	title = "EVTNAME880190"
	desc = "EVTDESC880190"
	picture = "news"
	
	trigger = {
		civilized = yes
		is_ideology_enabled = traditionalist
		NOT = {
			publishing_rights = censored_publishing
			political_parties = parties_not_allowed
			has_country_modifier = ruling_party_scandal
			has_country_modifier = party_scandal_evidence
		}
		OR = {
			traditionalist = 10
			any_owned_province = { traditionalist = 20 }
		}
	}
	
	mean_time_to_happen = {
		months = 400
		modifier = {
			factor = 0.75
			election = yes
		}
		modifier = {
			factor = 1.5
			ruling_party_ideology = traditionalist
			NOT = { publishing_rights = unregulated_publishing }
		}
		modifier = {
			factor = 0.5
			election = yes
			ruling_party_ideology = traditionalist
		}
		modifier = {
			factor = 1.5
			NOT = { average_consciousness = 2 }
		}
		modifier = {
			factor = 1.5
			NOT = { average_consciousness = 4 }
		}
		modifier = {
			factor = 0.8
			average_consciousness = 6
		}
		modifier = {
			factor = 0.8
			average_consciousness = 8
		}
		modifier = {
			factor = 0.8
			average_consciousness = 9
		}
		modifier = {
			factor = 0.8
			traditionalist = 25
		}
		modifier = {
			factor = 0.8
			traditionalist = 50
		}
		modifier = {
			factor = 0.8
			traditionalist = 75
		}	
		modifier = {
			factor = 1.2
			NOT = {	ruling_party_ideology = traditionalist }
		}
		modifier = {
			factor = 0.7
			NOT = { ruling_party_ideology = traditionalist }
			political_parties = parties_only_nominated
		}
		modifier = {
			factor = 0.8
			NOT = { ruling_party_ideology = traditionalist }
			political_parties = parties_allowed
		}
		modifier = {
			factor = 0.9
			NOT = { ruling_party_ideology = traditionalist }
			political_parties = parties_promote_democratic
		}
		modifier = {
			factor = 0.9
			NOT = { ruling_party_ideology = traditionalist }
			publishing_rights = firewall_publishing
		}
		modifier = {
			factor = 1.5
			ruling_party_ideology = traditionalist
			political_parties = parties_only_nominated
		}
		modifier = {
			factor = 1.2
			ruling_party_ideology = traditionalist
			political_parties = parties_allowed
		}
		modifier = {
			factor = 1.1
			ruling_party_ideology = traditionalist
			political_parties = parties_promote_democratic
		}
		modifier = {
			factor = 0.5
			ruling_party_ideology = traditionalist
			has_country_modifier = massive_government_corruption
		}
		modifier = {
			factor = 0.7
			ruling_party_ideology = traditionalist
			has_country_modifier = large_government_corrupttion
		}
		modifier = {
			factor = 0.9
			ruling_party_ideology = traditionalist
			has_country_modifier = medium_government_corruption
		}
		modifier = {
			factor = 0.9
			ruling_party_ideology = traditionalist
			has_country_modifier = mnc_corruption
		}
		modifier = {
			factor = 0.8
			ruling_party_ideology = traditionalist
			has_country_modifier = mnc_corrupt_leadership
		}
	}
	
	option = {
		name = "EVTOPTA880190"
		random_owned = {
			limit = {
				owner = { ruling_party_ideology = traditionalist }
			}
			owner = {
				prestige = -5
				plurality = -1
				add_country_modifier = {
					name = ruling_party_scandal
					duration = 730
				}
			}
		}
		any_pop = {
			limit = {
				location = { is_colonial = no }
				traditionalist = 10
			}
			ideology = {
				value = traditionalist
