#cweCommunists in $PROVINCENAME$
province_event = {
	id = 17730
	title = "EVTNAME17730"
	desc = "EVTDESC17730"
	
	trigger = {
		is_ideology_enabled = communist
		is_colonial = no
		owner = { NOT = {	OR = { pop_majority_religion = sunni pop_majority_religion = shiite } } }
		NOT = { controlled_by = REB }
		OR = {
			socialist = 20
			communist = 10
		}
		OR = {
			has_pop_type = artisans
			has_pop_type = clerks
			has_pop_type = craftsmen
			has_pop_type = labourers
		}
		OR = {
			average_militancy = 6
			has_province_modifier = militant_trade_union
			has_province_modifier = province_strike
			NOT = { poor_strata_life_needs = 0.7 }
		}
		owner = {
			NOT = {
				government = proletarian_dictatorship
				government = proletarian_dictatorship1
				government = socialist_democracy
				government = theocracy
			}
			civilized = yes
			civil_service = elitist_service
		}
	}
	
	mean_time_to_happen = {
		months = 200
		modifier = {
			factor = 0.5
			OR = {
				has_province_modifier = militant_trade_union
				has_province_modifier = province_strike
			}
		}
		modifier = {
			factor = 1.5
			NOT = { socialist = 10 }
		}
		modifier = {
			factor = 1.5
			NOT = { communist = 5 }
		}
		modifier = {
			factor = 0.9
			socialist = 25
		}
		modifier = {
			factor = 0.9
			socialist = 30
		}
		modifier = {
			factor = 0.9
			communist = 15
		}
		modifier = {
			factor = 0.9
			communist = 20
		}
		modifier = {
			factor = 0.8
			communist = 25
		}
		modifier = {
			factor = 0.9
			average_militancy = 4
		}
		modifier = {
			factor = 0.9
			average_militancy = 5
		}
		modifier = {
			factor = 0.9
			average_militancy = 6
		}
		modifier = {
			factor = 1.5
			NOT = { average_consciousness = 3 }
		}
		modifier = {
			factor = 0.9
			average_consciousness = 5
		}
		modifier = {
			factor = 0.9
			average_consciousness = 7
		}
		modifier = {
			factor = 1.2
			owner = { economic_policy = laissez_faire }
		}
		modifier = {
			factor = 2
			owner = { war = yes }
		}
		modifier = {
			factor = 1.5
			owner = {
				check_variable = {
					which = successful_communist_rebellions
					value = 10
				}
			}
		}
		modifier = {
			factor = 0.8
			any_neighbor_province = {
				owner = { government = proletarian_dictatorship }
			}
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 50 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 45 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 40 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 35 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 30 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 25 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 20 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 15 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 10 }
		}
		modifier = {
			factor = 1.5
			owner = { num_of_cities = 5 }
		}
	}
	
	option = {
		name = "EVTOPTA17730"
		random_pop = {
			limit = {
				strata = poor
				OR = {
					socialist = 20
					communist = 10
				}
			}
			reduce_pop = 0.98
			militancy = 4
		}
		poor_strata = {
			militancy = 2
		}
		rich_strata = {
			ideology = {
				value = socialist
				factor = -0.1
			}
			ideology = {
				value = communist
				factor = -0.2
			}
			ideology = {
				value = populist
				factor = 0.2
			}
			militancy = -3
			consciousness = -3
		}	
	}
	
	option = {
		name = "EVTOPTB17730"
		state_scope = {
			any_pop = {
				limit = {
					OR = {
						type = artisans
						type = clerks
						type = labourers
						type = craftsmen
					}
				}
				ideology = {
					value = communist
					factor = 0.1
				}
				scaled_consciousness = {
					ideology = communist
					factor = 4
				}
			}
			rich_strata = {
				militancy = 3
			}
		}
	}
}
#cwe Virgin Lands Campaign - Poor Results
country_event = {
	id = 9001022
	title = "Virgin Lands Campaign - Poor Results"
	desc = EVT_9001022_DESC
	picture = "virgin_land_campaign_fail"
	
	trigger = {
		tag = RUS
		government = proletarian_dictatorship
		has_country_flag = virgin_lands_campaign_bad
	}
	
	mean_time_to_happen = { months = 12 }
	
	option = {
		name = "Some people thank the $MONARCHTITLE$"
		any_pop = { 
			militancy = 2
			consciousness = 2
			ideology = { factor = -0.02 value = communist }
		}
		
		add_country_modifier = {
			name = economic_malaise
			duration = 365
		}
		
		prestige = -10
		
		clr_country_flag = virgin_lands_campaign_bad
	}
}	
