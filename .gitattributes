	country_event = {
		id = 99944
		title = "EVTNAME99944"
		desc = "EVTDESC99944"
		fire_only_once = yes
		major = yes

		trigger = {
			tag = FRA
			government = democracy
			election = no
			412 = { NOT = { owned_by = THIS } }
			has_country_flag = boulanger_coup_fails
			NOT = { has_country_flag = boulanger_exiled }
		}

		mean_time_to_happen = {
			days = 7
		}

		immediate = { set_country_flag = boulanger_exiled }

		option = {
			name = "EVTOPTA99944"

			any_pop = {
				limit = {
					is_primary_culture = yes
					is_accepted_culture = yes
				}
				ideology = { factor = -0.2 value = reactionary }
				ideology = { factor = -0.1 value = conservative }
			}

			any_pop = {
				limit = {
					location = { is_core = THIS }
					is_primary_culture = yes
				}
				scaled_militancy = { ideology = reactionary factor = 4 }
				scaled_militancy = { ideology = conservative factor = 4 }
				scaled_militancy = { ideology = liberal factor = -4 }
				scaled_militancy = { ideology = socialist factor = -4 }
				scaled_militancy = { ideology = communist factor = -4 }
			}
		}
